{
  "coc.preferences.diagnostic.displayByAle": true,
  "eslint.autoFixOnSave": true,
  "languageserver": {
    "sorbet": {
      "args": [
        "tc",
        "--lsp",
        "--enable-all-experimental-lsp-features",
        "--disable-watchman"
      ],
      "trace.server": "verbose",
      "command": "./bin/srb",
      "filetypes": [
        "ruby"
      ],
      "rootPatterns": [
        "sorbet/config"
      ]
    }
  }
}
