{
  "suggest.enablePreview": true,
  "codeLens.enable": false,
  "tsserver.enableJavascript": true,
  "eslint.autoFixOnSave": true,
  "eslint.quiet": true,
  "languageserver": {
    "sorbet": {
      "args": [
        "tc",
        "--lsp",
        "--enable-all-experimental-lsp-features",
        "--disable-watchman"
      ],
      "trace.server": "verbose",
      "command": "./bin/srb",
      "filetypes": [
        "ruby"
      ],
      "rootPatterns": [
        "sorbet/config"
      ]
    }
  }
}
